{
  "scripts": {
    "cy-open": "cypress open --reporter mochawesome",
    "cy-run": "cypress run --browser chrome --headed --reporter mochawesome",
    "mocha-merge": "npx mochawesome-merge ./cypress/reports/*.json -o mochawesome.json",
    "mocha-export": "npx marge mochawesome.json",
    "mocha-show": "cmd /c start mochawesome-report/mochawesome.html"
  },
  "dependencies": {
    "@cypress/xpath": "^2.0.3",
    "cypress": "12.7.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^4.9.5"
  }
}
